<div class="container">
	<div class="row">
		<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Mobile</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
  	<%@users.each do |user|%>
    <tr>
      <td><%=user.name%></td>
      <td><%=user.email%></td>
      <td><%=user.mobile%></td>
      <td><%= link_to "Edit", user_path(user), class: "nav-link" %></td>
    </tr>
    <%end%>
  </tbody>
</table>
		
	</div>
	
</div>